{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "google"
    ],
    "plugins":[
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": [2, {"code": 120, "tabWidth": 2, "ignoreUrls": true}],
        "new-cap": 0
    }
}
